{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "jsx": "preserve",
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@/components/*": ["./src/components/*"],
            "@/utils/*": ["./src/utils/*"],
            "@/layout/*": ["./src/layout/*"],
            "@/pages/*": ["./src/pages/*"],
            "@/apis/*": ["./src/apis/*"]
        }
    }
}
